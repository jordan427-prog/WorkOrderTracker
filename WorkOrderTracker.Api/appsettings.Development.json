{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "Default": "Server=(localdb)\\MSSQLLocalDB;Database=WorkOrderTracker;Trusted_Connection=True;TrustServerCertificate=True"
  }

}
/*
  (localdb)\MSSQLLocalDB is your local SQL Server instance

Database=WorkOrderTracker will create/use a database with that name

Trusted_Connection=True means Windows auth, no password needed
  */
